{
  "name": "@pawpal/nextjs-middleware",
  "version": "1.0.0",
  "description": "A flexible, Laravel-inspired middleware system for Next.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "nextjs",
    "middleware",
    "authentication",
    "authorization",
    "rate-limiting",
    "security",
    "caching",
    "typescript"
  ],
  "author": "PawPal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pawpal/nextjs-middleware.git"
  },
  "bugs": {
    "url": "https://github.com/pawpal/nextjs-middleware/issues"
  },
  "homepage": "https://github.com/pawpal/nextjs-middleware#readme",
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "next": "catalog:",
    "react": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "typescript": "catalog:",
    "jest": "catalog:",
    "@types/jest": "catalog:",
    "eslint": "catalog:",
    "@pawpal/eslint-config": "workspace:*",
    "@pawpal/typescript-config": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
