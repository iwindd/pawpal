{
  "name": "@pawpal/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./*": "./src/*.tsx",
    "./components/*": "./src/components/*/index.tsx",
    "./providers/*": "./src/providers/*.tsx",
    "./configs/*": "./src/configs/*.tsx",
    "./styles/*": "./src/styles/*",
    "./layouts/*": "./src/layouts/*.tsx",
    "./types": "./src/type.d.ts"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@pawpal/eslint-config": "workspace:*",
    "@pawpal/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "catalog:",
    "postcss": "catalog:",
    "postcss-preset-mantine": "catalog:",
    "postcss-simple-vars": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@mantine/core": "catalog:",
    "@mantine/hooks": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  }
}
